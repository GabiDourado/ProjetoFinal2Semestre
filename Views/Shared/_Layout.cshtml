<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - ProjetoFinal2Semestre</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" asp-append-version="true" />
    <link rel="stylesheet" href="~/ProjetoFinal2Semestre.styles.css" asp-append-version="true" />
    <link rel="preconnect" href="https://fonts.googleapis.com">
    <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
    <link href="https://fonts.googleapis.com/css2?family=Itim&display=swap" rel="stylesheet">
</head>
<body>
    <header>
        <nav class="navbar-expand-sm navbar-toggleable-sm border-bottom box-shadow mb-3">
            <div class="container-fluid">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index"><img src="~/css/Logo.png" /></a>
                <button class="navbar-toggler" type="button" data-bs-toggle="collapse" data-bs-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="volta-menu navbar-collapse ">
                    <ul class="menu">
                        <li class="Item1">
                            <a  asp-area="" asp-controller="Usuario" asp-action="Index">Usuários</a>
                        </li>
                        <li class="Item">
                            <a  asp-area="" asp-controller="Cliente" asp-action="Index">Cliente</a>
                        </li>
                        <li class="Item">
                            <a  asp-area="" asp-controller="Produto" asp-action="Index">Produtos</a>
                        </li>
                        <li class="Item">
                            <a  asp-area="" asp-controller="TipoProduto" asp-action="Index">Tipo de Produto</a>
                        </li>
                        <li class="Item">
                            <a  asp-area="" asp-controller="Fornecedor" asp-action="Index">Fornecedor</a>
                        </li>
                        <li class="Item">
                            <a  asp-area="" asp-controller="Cidade" asp-action="Index">Cidade</a>
                        </li>
                        <li class="Item">
                            <a  asp-area="" asp-controller="Estado" asp-action="Index">Estado</a>
                        </li>
                        <li class="Item">
                            <a  asp-area="" asp-controller="Pais" asp-action="Index">País</a>
                        </li>
                        <li class="Item">
                            <a  asp-area="" asp-controller="EntradaProduto" asp-action="Index">Entrada de Produtos</a>
                        </li>
                        <li class="Item">
                            <a  asp-area="" asp-controller="SaidaProduto" asp-action="Index">Saída de produtos</a>
                        </li>
                        <li class="Item">
                            <a  asp-area="" asp-controller="TipoSaida" asp-action="Index">Tipo de saída</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="rodape">
            Controle de vendas e movimentação de produtos
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
